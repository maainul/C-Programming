#include<stdio.h>
int main()
{
    int a;
    float b;
    char c;
    printf("Enter values of a, b and c\n");
    if(scanf("%d %f %c",&a,&b,&c)==3)
        printf("a=%d b=%f c=%c\n",a,b,c);
    else
        printf("Error in input");
    return 0;
}

### Output###
Enter values of a, b and c
4 68.00 anik
a=4 b=68.00 c=a

