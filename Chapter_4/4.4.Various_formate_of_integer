#include<stdio.h>
int main()
{
    int a,b,c,x,y,z;
    int p,q,r;
    printf("Enter the three integer number \n");
    scanf("%d %*d %d",&a,&b,&c);
    printf("%d %d %d\n\n",a,b,c);//For the * c contain garbage value

    printf("Enter two integer\n");
    scanf("%2d %4d",&x,&y);
    printf("%d %d\n\n",x,y);

    return 0;
}
