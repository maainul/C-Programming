#include<stdio.h>
void main()
{

    int c;
    double f;
    printf("Enter The Celcius :");
    scanf("%d",&c);
    f=(9*c)/5+32;
    printf("Celicius to Farenhite :%lf F",f);
    int d;
    double x;
    printf("\nEnter The Farenhite :");
    scanf("%d",&d);
    x=((d-32)*5)/9;
    printf("Farenhite to celcious : %lf c",x);


}
